{
  "name": "chat",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": ">=0.12"
  },
  "source": "static/index.html",
  "web": "dist/index.html",
  "targets": {
    "web": {
      "engines": {
        "browsers": [
          "> 1%",
          "not dead"
        ]
      }
    }
  },
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production",
    "server": "node server.js",
    "stylelint": "stylelint \"**/*.scss\" --fix",
    "eslint": "eslint \"**/*.ts\" --fix",
    "test": "mocha -r jsdom-global/register",
    "pre-commit": "npm run test && npm run eslint && npm run stylelint",
    "postinstall": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm-run pre-commit"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "chai": "^4.3.4",
    "css-loader": "^6.5.1",
    "eslint": "^8.1.0",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jsdom": "18.1.1",
    "jsdom-global": "3.0.2",
    "mocha": "^9.1.3",
    "sass": "^1.42.1",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.0.0",
    "stylelint-config-standard": "^23.0.0",
    "stylelint-config-standard-scss": "^2.0.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.67.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.7.3"
  },
  "dependencies": {
    "@types/uuid": "^8.3.1",
    "handlebars": "^4.7.7",
    "uuid": "^8.3.2"
  }
}
